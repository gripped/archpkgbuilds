From 9385d6f281426bf888a6df51c806877fd85ff056 Mon Sep 17 00:00:00 2001
From: Carl Smedstad <carl.smedstad@protonmail.com>
Date: Sat, 10 Aug 2024 12:17:17 +0200
Subject: [PATCH 07/12] Fix remaining compilation issues

---
 osquery/events/types.h       | 2 +-
 osquery/logger/data_logger.h | 2 +-
 osquery/logger/logger.cpp    | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/osquery/events/types.h b/osquery/events/types.h
index 554db70f1..334bf382d 100644
--- a/osquery/events/types.h
+++ b/osquery/events/types.h
@@ -21,7 +21,7 @@ namespace osquery {
 using EventContextID = uint64_t;
 using EventTime = uint64_t;
 using EventRecord = std::pair<std::string, EventTime>;
-using EventID = std::uint64_t;
+using EventID = uint64_t;
 using EventIDList = std::vector<EventID>;
 using EventIndex = std::map<EventTime, EventIDList>;
 
diff --git a/osquery/logger/data_logger.h b/osquery/logger/data_logger.h
index d5e38a6b3..9062a92e6 100644
--- a/osquery/logger/data_logger.h
+++ b/osquery/logger/data_logger.h
@@ -156,6 +156,6 @@ void systemLog(const std::string& line);
  * prefix. We want to use the old format without it for now.
  */
 void googleLogCustomPrefix(std::ostream& s,
-                           const LogMessageInfo& l,
+                           const google::LogMessageInfo& l,
                            void* data);
 } // namespace osquery
diff --git a/osquery/logger/logger.cpp b/osquery/logger/logger.cpp
index cddc89be9..353a40261 100644
--- a/osquery/logger/logger.cpp
+++ b/osquery/logger/logger.cpp
@@ -562,7 +562,7 @@ void systemLog(const std::string& line) {
 }
 
 void googleLogCustomPrefix(std::ostream& s,
-                           const LogMessageInfo& l,
+                           const google::LogMessageInfo& l,
                            void* data) {
   s << l.severity[0] << std::setw(2) << (l.time.month() + 1) << std::setw(2)
     << l.time.day() << ' ' << std::setw(2) << l.time.hour() << ':'
