---
 search-provider/lollypop-sp.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/search-provider/lollypop-sp.in b/search-provider/lollypop-sp.in
index 485c6a313..f37f4dd4d 100755
--- a/search-provider/lollypop-sp.in
+++ b/search-provider/lollypop-sp.in
@@ -226,7 +226,7 @@ class SearchLollypopService(Server, Gio.Application):
 
     def LaunchSearch(self, terms, utime):
         results = self.__search(terms)
-        argv = ["lollypop", "--play-ids", ";".join(results), None]
+        argv = ["lollypop", "--play-ids", ";".join(results)]
         (pid, stdin, stdout, stderr) = GLib.spawn_async(
                     argv, flags=GLib.SpawnFlags.SEARCH_PATH,
                     standard_input=False,
