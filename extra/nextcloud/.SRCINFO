pkgbase = nextcloud
	pkgdesc = A cloud server to store your files centrally on a hardware controlled by you
	pkgver = 31.0.4
	pkgrel = 1
	url = https://nextcloud.com
	install = nextcloud.install
	arch = any
	license = AGPL-3.0-or-later
	depends = php-legacy-gd
	depends = php-legacy
	optdepends = curl: for webcron jobs from within uwsgi
	optdepends = ffmpeg: for preview generation
	optdepends = libreoffice: for preview generation
	optdepends = mariadb: to use a local MariaDB server
	optdepends = memcached: to use a local memcached server
	optdepends = mod_itk: to use the Apache web server
	optdepends = php-legacy-apache: to use the Apache web server
	optdepends = php-legacy-apcu: for caching with APCU
	optdepends = php-legacy-fpm: to run in fastCGI process manager
	optdepends = php-legacy-imagick: for preview generation
	optdepends = php-legacy-memcached: for caching with memcached
	optdepends = php-legacy-pgsql: to use the PostgreSQL database backend
	optdepends = php-legacy-redis: for caching with redis
	optdepends = php-legacy-sodium: for Argon2 password hashing
	optdepends = php-legacy-sqlite: to use the SQLite database backend
	optdepends = postgresql: to use a local PostgreSQL server
	optdepends = valkey: to use a local Redis server
	optdepends = smbclient: for SMB/CIFS integration
	optdepends = sudo: for privilege elevation in occ command when not run as root
	optdepends = uwsgi-plugin-php-legacy: run as application container
	options = !strip
	backup = etc/webapps/nextcloud/config/config.php
	backup = etc/webapps/nextcloud/.htaccess
	backup = etc/uwsgi/nextcloud.ini
	source = https://download.nextcloud.com/server/releases/nextcloud-31.0.4.tar.bz2
	source = https://download.nextcloud.com/server/releases/nextcloud-31.0.4.tar.bz2.asc
	source = apache.example.conf
	source = nextcloud.config.php
	source = nextcloud.hook
	source = nextcloud.occ.sh
	source = nextcloud.sysusers
	source = nextcloud.tmpfiles
	source = nextcloud.uwsgi
	source = nextcloud-cron.service
	source = nextcloud-cron.timer
