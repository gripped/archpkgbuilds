function _package() {
  local fontname=${1:-}
  install -vDm644 -t "$pkgdir/usr/share/fonts/TTF" "Iosevka$fontname"-*.ttc
  [ -n "$fontname" ] && pkgdesc+=" ($fontname variant)"
  install -vDm644 "$pkgbase-$pkgver-$pkgrel-LICENSE.md" \
    "$pkgdir/usr/share/licenses/$pkgname/LICENSE.md"
}

function package_ttc-iosevka() {
  _package
}

function package_ttc-iosevka-aile() {
  _package Aile
